{
  "name": "visearch-javascript-sdk",
  "version": "4.0.0-rc.0",
  "description": "JavaScript SDK for ViSearch of visenze.com",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js",
      "default": "./cjs/index.js"
    }
  },
  "scripts": {
    "lint": "eslint src --ext .ts",
    "start-demo": "npm run build-dev && node examples/app.js",
    "write-version": "node -p \"'export const version =  \\'' + require('./package.json').version + '\\';'\" > src/version.ts",
    "build-pkg": "npm run write-version && npm run build-cjs && npm run build-esm && ./fixup",
    "build-cjs": "rm -fr cjs/* && tsc -p tsconfig.cjs.json",
    "build-esm": "rm -fr esm/* && tsc -p tsconfig.json",
    "build-dev": "webpack --mode development  --config webpack.config.js",
    "build": "npm run write-version && webpack --config webpack.config.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visenze/visearch-sdk-javascript.git"
  },
  "author": "dejun@visenze.com, rachel.ngo@visenze.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@types/jsuri": "^1.3.31",
    "@types/lodash.find": "^4.6.7",
    "@types/lodash.isfunction": "^3.0.7",
    "@types/node-fetch": "^2.6.4",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.3.0",
    "compression-webpack-plugin": "^10.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.40.0",
    "express": "^4.18.2",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.8",
    "terser-webpack-plugin": "^5.3.8",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1",
    "webpack-s3-plugin": "^1.2.0-rc.0"
  },
  "dependencies": {
    "form-data": "^2.3.3",
    "jsuri": "^1.3.1",
    "lodash.find": "^4.6.0",
    "lodash.isfunction": "^3.0.9",
    "node-fetch": "^2.6.11",
    "visenze-tracking-javascript": "^2.0.0-rc.2"
  }
}
