{
  "name": "visearch-javascript-sdk",
  "version": "4.0.0",
  "description": "JavaScript SDK for ViSearch of visenze.com",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "start-demo": "npm run build-dev && node examples/app.js",
    "get-version": "node -p \"'export const version =  \\'' + require('./package.json').version + '\\';'\" > src/version.ts",
    "build-dev": "webpack --mode development  --config webpack.config.js",
    "build": "npm run get-version && webpack --config webpack.config.js",
    "publish-npm": "npm run get-version && npx tsc && npm publish",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/visenze/visearch-sdk-javascript.git"
  },
  "author": "dejun@visenze.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@types/jsuri": "^1.3.31",
    "@types/lodash.find": "^4.6.7",
    "@types/lodash.isfunction": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.3.0",
    "compression-webpack-plugin": "^10.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.40.0",
    "exports-loader": "^0.7.0",
    "express": "^4.18.2",
    "imports-loader": "^0.8.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.1.1",
    "terser-webpack-plugin": "^5.3.8",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1",
    "webpack-s3-plugin": "^1.2.0-rc.0"
  },
  "dependencies": {
    "form-data": "^2.3.3",
    "jsuri": "^1.3.1",
    "lodash.find": "^4.6.0",
    "lodash.isfunction": "^3.0.9",
    "node-fetch": "^2.6.11",
    "visearch-javascript-sdk": "^4.0.0-beta.2",
    "visenze-tracking-javascript": "1.2.1"
  }
}